{
    "compilerOptions": {
        "outDir": "build",
        "target": "es5",
        "module": "commonjs",
        "sourceMap": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true
    },
    "typeRoots": [
        "node_modules/@types",
        "src/types"
    ],
}